generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model FileData {
  id    String    @id
  MsgId String
  Size  String
  Name  String
  Type  String
  UploaderId String
  Link String
  DownloadLink String
  CreatedAt String   
}

model User {
  id       Int      @id @default(autoincrement())
  DCID String
  username String
  RegisterAt DateTime @default(now())
}


